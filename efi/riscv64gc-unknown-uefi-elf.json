{
  "arch": "riscv64",
  "code-model": "medium",
  "cpu": "generic-rv64",
  "crt-objects-fallback": "false",
  "data-layout": "e-m:e-p:64:64-i64:64-i128:128-n32:64-S128",
  "disable-redzone": true,
  "eh-frame-header": false,
  "emit-debug-gdb-scripts": false,
  "exe-suffix": ".efi",
  "features": "+m,+a,+f,+d,+c",
  "linker": "rust-lld",
  "linker-flavor": "gnu-lld",
  "llvm-abiname": "lp64d",
  "llvm-target": "riscv64-unknown-none-elf",
  "max-atomic-width": 64,
  "metadata": {
    "description": "RISC-V64 UEFI",
    "host_tools": false,
    "std": null,
    "tier": 3
  },
  "os": "none",
  "panic-strategy": "abort",
  "position-independent-executables": true,
  "pre-link-args": {
    "gnu-lld": [
      "-Bstatic",
      "--no-dynamic-linker",
      "--eh-frame-hdr",
      "--no-rosegment",
      "-ztext",
      "-znorelro",
      "--entry=efi_main"
    ]
  },
  "late-link-args": {
    "gnu-lld": [
      "--gc-sections"
    ]
  },
  "relocation-model": "pic",
  "singlethread": true,
  "stack-probes": {
    "kind": "none"
  },
  "supported-sanitizers": [],
  "target-pointer-width": "64"
}